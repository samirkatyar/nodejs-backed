{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["src/**/*.(t|j)s"],
  "coverageDirectory": "test/coverage/",
  "coveragePathIgnorePatterns": [
    "<rootDir>/src/migrations/",
    ".spec.ts",
    "main.ts"
  ]
}
